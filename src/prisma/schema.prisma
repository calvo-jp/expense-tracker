datasource db {
  provider = "mongodb"
  url      = env("MONGODB_URI")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  username String @unique
  password String
}

model Expense {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  what      String
  where     String
  when      DateTime
  amount    Float
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
